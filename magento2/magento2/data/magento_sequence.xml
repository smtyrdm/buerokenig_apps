<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="magento_website_sequence" model="ir.sequence">
            <field name="name">Website Number</field>
            <field name="code">magento.website</field>
            <field name="active">TRUE</field>
            <field name="prefix">WN/</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
    </data>
    <data noupdate="1">
        <record id="magento_stores_sequence" model="ir.sequence">
            <field name="name">Store Number</field>
            <field name="code">magento.stores</field>
            <field name="active">TRUE</field>
            <field name="prefix">SN/</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>