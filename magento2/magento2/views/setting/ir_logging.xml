<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="magento_logging_action" model="ir.actions.act_window">
            <field name="name">Logging Magento</field>
            <field name="res_model">ir.logging</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_no content_create">
                    Click to create a Log.
                </p>
            </field>
            <field name="domain">[('name', 'ilike', 'magento')]</field>
            <field name="context">{'default_name': 'magento'}</field>
        </record>
    </data>
</odoo>
